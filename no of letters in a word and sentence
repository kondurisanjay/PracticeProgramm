import java.util.*;
//no of letters in a word and sentence
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner (System.in);
        String s = sc.nextLine();
        
        int count = 0 ;
        
        for(int i = 1;i<=s.length();i++){
            count++;
        }
        System.out.println(count);
    }
}
